                                                                            classStr = "material_8";
                                                                        }
                                                                        else if (shares.DocType == "邀请证明")
                                                                        {
                                                                            classStr = "material_9";
                                                                        }
                                                                        else
                                                                        {
                                                                            classStr = "material_10";
                                                                        }



