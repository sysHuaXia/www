                                            @foreach (var initF in Model.FirstList.Keys)
                                            {
                                                string str = "";
                                                string mo = "#Section";
                                                if (ViewBag.No == 1) {
                                                    str = "active";
                                                }
                                                <li role="presentation" class="@str" style="font-size:12px;"><a href="@mo@ViewBag.No" aria-controls="home" role="tab" data-toggle="tab"><span>@initF.CrowdTypeName</span></a></li>
                                                ViewBag.No++;
                                            }

                                        </ul>
                                        <!-- Tab panes -->
                                        <div class="tab-content tabs">


                                            @foreach (var init in Model.FirstList.Keys) {

                                                string DocTypeStr = "";
                                                string str1 = "";
                                                string mo = "Section";
                                                if (ViewBag.Nu == 1)
                                                {
                                                    str1 = "active";
                                                }
                                                <div role="tabpanel" class="tab-pane fade in @str1" id="@mo@ViewBag.Nu">
