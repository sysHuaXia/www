@model CoreModels.Models.VisaInfoViewModel
@{
    Layout = null;
    ViewBag.Title = "AppIndex";
}

<html>
<head lang="en">
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no" />
    <title>签证进度查询</title>
    @*<link rel="stylesheet" href="css/common.css" />*@
    <link href="~/ClientWebJsCss/Css/login.css" rel="stylesheet" />
</head>
<body>
    <div id="zhuce" style="display:none;">注册</div>
    <div id="login"></div>
    <div class="login_bg">
        <div id="logo">

        </div>

            @using (Html.BeginForm("InAppInterface", "ClientWeb", FormMethod.Post))
            {
                @Html.AntiForgeryToken()
                <div class="userName">
                    <lable>+86&nbsp;</lable>
                    @Html.TextBoxFor(x => x.ClientInterface.v_tel, new { @class = "input_kuang", @placeholder = "联系人手机号" })
                </div>
                <div class="passWord">
                    <lable>&nbsp;&nbsp;&nbsp;</lable>
                    @Html.PasswordFor(x => x.ClientInterface.loginID, new { @class = "input_kuang", @placeholder = "订单号后6位数" })
                </div>
                <div class="choose_box">
                    <div>
                        <input type="checkbox" checked="checked" name="checkbox" />
                        <lable>记住密码</lable>
                    </div>
                    <a href="#">忘记密码</a>
                </div>
                <input type="submit" name="btnSign" class="login_btn" value="登录" class="no_bg" />
                @*<button class="login_btn" type="submit">登录</button>*@
            }

        <div class="other_login" style="display:none;">
            <span>点击登录按钮代表您已同意用户协议和隐私政策</span>
        </div>
        <div class="other_choose" style="display:none;">
            <a href="">
                <img src="~/ClientWebJsCss/Img/qq.png" />
            </a>
            <a href="">
                <img src="~/ClientWebJsCss/Img/wx.png" alt="" />
            </a>
            <a href="">
                <img src="~/ClientWebJsCss/Img/wb.png" alt="" />
            </a>
        </div>

    </div>
</body>
</html>

